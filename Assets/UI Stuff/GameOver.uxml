<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:VisualElement name="Background" style="flex-grow: 1; background-color: rgb(0, 0, 0); justify-content: center; align-items: center; display: none;">
        <ui:Button text="Try Again ?" display-tooltip-when-elided="true" name="RetryButton" style="border-left-width: 0; border-right-width: 0; border-top-width: 0; border-bottom-width: 0; border-top-left-radius: 3px; border-bottom-left-radius: 3px; border-top-right-radius: 3px; border-bottom-right-radius: 3px; background-color: rgb(42, 130, 71);" />
        <ui:Button text="Quit Game" display-tooltip-when-elided="true" name="QuitButton" style="height: 24px; width: 84px; border-left-width: 0; border-right-width: 0; border-top-width: 0; border-bottom-width: 0; border-top-left-radius: 3px; border-bottom-left-radius: 3px; border-top-right-radius: 3px; border-bottom-right-radius: 3px; background-color: rgb(142, 42, 42);" />
        <ui:Label text="Game Over" display-tooltip-when-elided="true" name="GameOverText" style="position: absolute; top: 20px; font-size: 30px; color: rgb(152, 2, 2);" />
    </ui:VisualElement>
</ui:UXML>
